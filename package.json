{
  "name": "nodeboilerplate",
  "version": "1.0.0",
  "description": "NodeJS boilerplate for Joonko's projects",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node .build/src/app.js",
    "build": "tsc --build",
    "pack": "zip -r <project_name>.zip package.json package-lock.json src",
    "test": "mocha --require ts-node/register --recursive --timeout 10000 test/**/**/*.ts --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/joonkodev/nodeboilerplate.git"
  },
  "keywords": [],
  "author": "Joonko",
  "license": "ISC",
  "homepage": "https://bitbucket.org/joonkodev/nodeboilerplate#readme",
  "dependencies": {
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "4.17.1",
    "express-validator": "6.6.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.12",
    "@types/supertest": "^2.0.12",
    "mocha": "8.4.0",
    "moxios": "0.4.0",
    "nodemon": "2.0.12",
    "rewire": "5.0.0",
    "should": "13.2.3",
    "sinon": "9.2.3",
    "supertest": "4.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.2"
  }
}
